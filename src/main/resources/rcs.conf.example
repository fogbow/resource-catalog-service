# ----- Fill the required information and move it to ./private/rcs.conf -----

# Membership Service configuration
# Required
ms_url=http://localhost
# Required
ms_ip=127.0.0.1
# Required
ms_port=8083

# Resource Allocation Service configuration
# Required
ras_ip=127.0.0.1
# Required
ras_port=8080

# Authentication Service configuration
# Required
as_ip=127.0.0.1
# Required
as_port=8081

# Federated Network Service configuration
# Required
fns_ip=127.0.0.1
# Required
fns_port=8082

# XMPP configurations
# Required
provider_id=local-provider-member-id
# Required
xmpp_password=pass
# Required
xmpp_server_ip=127.0.0.1
# Not required
xmpp_c2c_port=
# Not required
xmpp_timeout=

# Catalog Members File
# Required
members_file_path=

# Cache Validity in minutes
# Optional
cache_validity=